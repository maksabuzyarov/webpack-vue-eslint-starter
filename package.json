{
  "name": "webpack-vue-eslint-starter",
  "version": "1.1.0",
  "description": "Webpack frontend builder with eslint and Vue support",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js --progress",
    "build-describe": "webpack --config webpack.prod.js --progress --display-modules",
    "lint": "eslint . --fix"
  },
  "keywords": [
    "webpack",
    "frontend",
    "babel",
    "vue",
    "eslint",
    "foundation"
  ],
